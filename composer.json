{
  "name": "voodooism/stega-php",
  "license": "MIT",
  "type": "library",
  "description": "PHP library for image steganography",
  "require": {
    "paragonie/seedspring": "^1.1",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "symfony/polyfill-php74": "^1.15"
  },
  "autoload": {
    "psr-4": {
      "Voodooism\\Stega\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Voodooism\\Stega\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.5",
    "vimeo/psalm": "^3.11"
  }
}
